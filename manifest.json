{
    "manifest_version": 2,

    "name": "Listify",
    "description": "Bytedance's Listify Tool, by kongyifei",
    "version": "1.0.0",

    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_icon": "icon.png"
    },

    "icons": {
        "128": "icon.png"
    },

    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs"
    ]
}
